{
  "name": "bybit-addresses",
  "version": "1.0.0",
  "description": "Playwright script for adding verified addresses to Bybit",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "serve": "npx ts-node index.ts",
    "dev": "nodemon index.ts --ignore '*.json'",
